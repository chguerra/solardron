<div class="container">
	<h5>Nuevo cliente</h5>
	<%= form_for @client do |f| %>
	<label>Nombre o Razón Social</label>
	<%= f.text_field :name %>
	<label>Dirección</label>
	<%= f.text_field :address %>
	<button type="submit" class="btn btn-success">
    	Añadir cliente
  	</button>
  	<% end %>
</div>